#include<stdio.h>
#include<stdlib.h>
int main()
{
int *ptr;
ptr=(int*)malloc(5*sizeof(int));
if(ptr==NULL)
{
printf("Memory is not allocated.\n");
}
else
printf("Memory allocated successfully.\n");
for(int i=0; i<5;i++)
{
ptr[i]=i+1;
}
printf("Numbers= ");
for(int i=0;i<5;i++)
{
printf("%d\t", ptr[i]);
}
free(ptr);
return 0
}
